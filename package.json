{
  "name": "residentprogram",
  "version": "2.0.0",
  "description": "Trainee evaluation system.",
  "author": "Jacob Mischka <jacob@mischka.me>",
  "repository": {
    "url": "git@github.com:jacobmischka/residentprogram.git",
    "type": "git"
  },
  "private": true,
  "scripts": {
    "start": "yarn watch",
    "up": "cd laradock && docker-compose up -d apache2 mysql",
    "shell": "cd laradock && docker-compose exec --user=laradock workspace bash",
    "down": "cd laradock && docker-compose down",
    "build": "run-s build:webpack wait build:gulp",
    "rebuild": "run-s remake build",
    "build:webpack": "NODE_ENV=production webpack -p --mode production",
    "build:gulp": "DISABLE_NOTIFIER=true gulp --production",
    "watch": "run-p watch:*",
    "watch:webpack": "webpack --colors --watch --mode development",
    "watch:gulp": "DISABLE_NOTIFIER=true gulp watch",
    "rewatch": "run-s remake watch",
    "clean": "rm -f public/js/* && rm -f public/css/*.css && rm -f public/css/*.map && rm -f public/build/js/* && rm -f public/build/css/*.css && rm -f public/build/css/*.map && rm -f public/build/rev-manifest.json",
    "wait": "sleep 2",
    "make": "run-s make:webpack wait make:gulp",
    "make:webpack": "webpack --mode development",
    "make:gulp": "DISABLE_NOTIFIER=true gulp",
    "remake": "run-s clean make",
    "analyze": "webpack-bundle-analyzer ./public/build/stats.json --port 8088 --no-open",
    "lint": "eslint resources/assets/js --ext .js,.vue && flow",
    "test": "run-s test:*",
    "test:backend": "./vendor/bin/phpunit",
    "test:frontend": "jest",
    "ui-test": "webpack-dev-server --config webpack.ui-tester.config.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "dependencies": {
    "@jacobmischka/transform-color": "^1.0.0",
    "@jacobmischka/vue-flatpickr": "^7.0.0",
    "ajv": "^6.4.0",
    "bootstrap-sass": "^3.0.0",
    "chart.js": "^2.2.0-rc.2",
    "classlist-polyfill": "^1.2.0",
    "color": "^3.0.0",
    "commonmark": "^0.28.1",
    "dlv": "^1.1.0",
    "downloadjs": "^1.4.6",
    "element-dataset": "^2.2.2",
    "element-resize-detector": "^1.1.12",
    "iframe-resizer": "^3.5.5",
    "indefinite": "^2.1.1",
    "intl": "^1.2.5",
    "jsonschema": "^1.1.0",
    "jszip": "^3.1.3",
    "ky": "^0.3.0",
    "localforage": "^1.5.0",
    "lodash": "^4.17.2",
    "lunr": "^2.3.3",
    "markdown-it": "^8.2.2",
    "medium-editor": "^5.23.1",
    "moment": "^2.14.1",
    "pdfmake": "^0.1.28",
    "raf": "^3.3.2",
    "randomcolor": "^0.5.3",
    "require-dir": "^1.0.0",
    "rollbar": "^2.2.10",
    "snarkdown": "^1.2.2",
    "striptags": "^3.1.1",
    "twix": "^1.1.3",
    "url-search-params-polyfill": "^5.0.0",
    "v-tooltip": "^2.0.0-beta.5",
    "vue": "2.5.17",
    "vue-router": "^3.0.1",
    "vue-select": "^2.5.1",
    "vue2-editor": "^2.4.2",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "apply-loader": "^2.0.0",
    "babel-eslint": "^9.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-module-resolver": "^3.0.0-beta.5",
    "babel-plugin-transform-vue-jsx": "^4.0.0",
    "bower": "^1.8.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-compat": "^2.0.0",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-vue": "^4.5.0",
    "flow-bin": "^0.81.0",
    "flow-typed": "^2.1.5",
    "git-rev-sync": "^1.12.0",
    "glob": "^7.1.1",
    "glow": "^1.2.2",
    "gulp": "^3.9.0",
    "gulp-each": "^0.5.0",
    "gulp-file-contents-to-json": "^0.2.1",
    "gulp-merge-json": "^1.0.0",
    "gulp-size": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "laravel-elixir": "^6.0.0-15",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "^4.0.0",
    "raw-loader": "^0.5.1",
    "rollbar-sourcemap-webpack-plugin": "^2.4.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "vue-eslint-parser": "^3.2.2",
    "vue-loader": "^15.0.5",
    "vue-template-compiler": "2.5.17",
    "vue-test-utils": "^1.0.0-beta.2",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^\\./resources/assets/js/(.*)$": "<rootDir>/resources/assets/js/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    }
  }
}
