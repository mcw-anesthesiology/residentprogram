{
  "name": "residentprogram",
  "version": "2.0.0",
  "description": "Trainee evaluation system.",
  "author": "Jacob Mischka <jacob@mischka.me>",
  "repository": {
    "url": "git@github.com:jacobmischka/residentprogram.git",
    "type": "git"
  },
  "private": true,
  "scripts": {
    "start": "yarn watch",
    "build": "run-s schemas build:webpack wait build:gulp",
    "build:webpack": "NODE_ENV=production webpack -p",
    "build:gulp": "gulp --production",
    "schemas": "rm -f public/schemas/* ; cp resources/assets/schemas/* public/schemas/",
    "watch": "run-p watch:*",
    "watch:webpack": "webpack --progress --colors --watch",
    "watch:gulp": "DISABLE_NOTIFIER=true gulp watch",
    "rewatch": "run-s remake watch",
    "clean": "rm -f public/js/* && rm -f public/css/*.css && rm -f public/css/*.map && rm -f public/build/js/* && rm -f public/build/css/*.css && rm -f public/build/css/*.map && rm -f public/build/rev-manifest.json",
    "wait": "sleep 2",
    "make": "run-s make:webpack wait make:gulp",
    "make:webpack": "webpack",
    "make:gulp": "gulp",
    "remake": "run-s clean make",
    "bundle": "webpack-bundle-analyzer ./public/build/js/stats.json",
    "lint": "eslint resources/assets/js --ext .js,.vue && flow",
    "test": "run-s test:*",
    "test:backend": "./vendor/bin/phpunit",
    "test:frontend": "jest",
    "ui-test": "webpack-dev-server --config webpack.ui-tester.config.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "dependencies": {
    "@jacobmischka/transform-color": "^1.0.0",
    "@jacobmischka/vue-flatpickr": "^3.0.1",
    "ajv": "^5.2.2",
    "bootstrap-sass": "^3.0.0",
    "chart.js": "^2.2.0-rc.2",
    "classlist-polyfill": "^1.0.3",
    "color": "^2.0.0",
    "commonmark": "^0.28.1",
    "dlv": "^1.1.0",
    "downloadjs": "^1.4.6",
    "element-dataset": "^2.2.2",
    "element-resize-detector": "^1.1.12",
    "iframe-resizer": "^3.5.5",
    "indefinite": "^1.0.1",
    "jsonschema": "^1.1.0",
    "jszip": "^3.1.3",
    "localforage": "^1.5.0",
    "lodash": "^4.17.2",
    "lunr": "^0.7.2",
    "markdown-it": "^8.2.2",
    "medium-editor": "^5.23.1",
    "moment": "^2.14.1",
    "pdfmake": "^0.1.28",
    "raf": "^3.3.2",
    "randomcolor": "^0.5.3",
    "require-dir": "^0.3.2",
    "snarkdown": "^1.2.2",
    "striptags": "^3.1.0",
    "twix": "^1.1.3",
    "v-tooltip": "^2.0.0-beta.5",
    "vue": "^2.4.2",
    "vue-router": "^3.0.1",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "apply-loader": "^2.0.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-module-resolver": "^3.0.0-beta.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.3",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^0.28.1",
    "eslint": "^4.5.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.5",
    "eslint-plugin-compat": "^2.0.0",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-vue": "^3.13.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.1.5",
    "glob": "^7.1.1",
    "gulp": "^3.9.0",
    "gulp-each": "^0.5.0",
    "gulp-file-contents-to-json": "^0.2.1",
    "gulp-merge-json": "^1.0.0",
    "gulp-size": "^2.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-vue": "^0.8.1",
    "laravel-elixir": "^6.0.0-15",
    "npm-run-all": "^4.0.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "vue-eslint-parser": "^2.0.1-beta.1",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.4.2",
    "vue-test-utils": "^1.0.0-beta.2",
    "webpack": "^3.4.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.9.3",
    "webpack-manifest-plugin": "^1.2.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^\\./resources/assets/js/(.*)$": "<rootDir>/resources/assets/js/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "mapCoverage": true
  }
}
